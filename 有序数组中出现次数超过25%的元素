class Solution {
    public int findSpecialInteger(int[] arr) {
        Map<Integer, Integer> map = new HashMap<>();
        for (int num : arr) {
            int count = map.getOrDefault(num, 0);
            map.put(num, count + 1);
        }
        //map中存储数组的元素以及个数
        for (int i = 0; i < arr.length; i++) {
            if (map.get(arr[i]) > arr.length / 4) {
                return arr[i];
            }
        }
        return -1;
    }
}

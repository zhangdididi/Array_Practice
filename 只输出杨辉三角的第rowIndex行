int* getRow(int rowIndex, int* returnSize){
    int *ret = malloc(sizeof(int) * (rowIndex+1));
    int i = 0;
    int j = 0;
    for(i = 0; i <= rowIndex; i++)
    {
        if(i <2)
        {
            ret[i] = 1;
        }
        else
        {
            ret[i] = 1;
            for(j = i-1; j >= 1;j--)
            {
                ret[j] = ret[j] + ret[j-1];
            }
        }
    }
    *returnSize = rowIndex+1;
    return ret;
}

class Solution {
    public int removeElement(int[] nums, int val) {
        //循环判断第i个元素是否为val，如果是val，让后一个元素等于这个元素
        //如果不是val，则两个下标向后移动
        int j = 0;
        for (int i = 0; i < nums.length; i++) {
            if (nums[i] != val) {
                nums[j++] = nums[i];
            }
        }
        return j;
    }
}

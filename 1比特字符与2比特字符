class Solution {
    public boolean isOneBitCharacter(int[] bits) {
        if (bits.length == 0) {
            return false;
        }
        int i = 0;
        while (i < bits.length - 1) {
            if (bits[i] == 0) {
                i++;
            } else if (bits[i] == 1) {
                i += 2;
            }
        }
        if (i == bits.length - 1) {
            return true;
        } else {
            return false;
        }
    }
}
